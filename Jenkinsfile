pipeline {
  agent any
  stages {
    stage('Tests') {
      steps {
        echo 'Démarrage du stage test'
        sleep 20
        echo 'Fin du step stage'
      }
    }

    stage('Build') {
      steps {
        echo 'start build stage'
        sleep 20
        echo 'End build stage'
      }
    }

    stage('Deploy') {
      steps {
        echo 'Start deploy stage'
        sleep 20
        echo 'End deploy stage'
      }
    }

  }
}